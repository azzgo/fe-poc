[ignore]
.*/__tests__/.*

[include]
.*/src/.*.js
.*/src/.*.jsx

[libs]
./flow-typed

[lints]
[options]
emoji=true
module.file_ext=.css
module.file_ext=.less
module.name_mapper.extension='css' -> 'CSSModule'
module.name_mapper.extension='scss' -> 'CSSModule'
module.name_mapper='^src/\(.*\)$' ->'<PROJECT_ROOT>/src/\1'

[strict]
