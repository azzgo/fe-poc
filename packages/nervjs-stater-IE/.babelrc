{
	"presets": [
		["env", {
			"spec": true,
			"useBuiltIns": false,
			"modules": false
		}],
		["es2015", {"loose": true}],
	],
	"plugins": [
		"transform-class-properties", 
		"transform-react-jsx",
		[
			"module-resolver", 
			{
				"root": ["."],
				"alias": {
					"react": "nervjs",
					"react-dom": "nervjs"
				}
			}
		],
		"transform-object-rest-spread",
		"transform-es3-member-expression-literals",
		"transform-es3-property-literals"
	]
}
