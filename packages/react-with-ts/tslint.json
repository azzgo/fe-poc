{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "interface-name": [true, "always-prefix"],
    "indent": [true, "spaces", 2],
    "no-unused-variable": [true, {"ignore-pattern": "^_"}]
  },
  "rulesDirectory": []
}
